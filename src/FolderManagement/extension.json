{
  "manifestVersion": 1,
  "id": null,
  "version": "1.1.0",
  "publisher": "alm-rangers",
  "extensionId": "almrangers-vsoextensions-FolderManagement",
  "name": "Folder Management",
  "description": "Enables you to create new folders from the code explorer within the Team Web Access.",
  "public": true,
  "icons": {
    "default": "images/VSO-Folder-196x.png"
  },
  "categories": [
    "Code"
  ],
  "tags": [
    "code",
    "folder folders",
    "directories directory",
    "create",
    "Git",
    "TFVC"
  ],
  "screenshots": [
    {
      "path": "screenshots/screen1.png"
    },
    {
      "path": "screenshots/screen2.png"
    }
  ],
  "content": {
    "details": {
      "path": "overview.md"
    },
    "license": {
      "path": "license.txt"
    }
  },
  "links": {
    "home": "http://aka.ms/vsoext01",
    "getstarted": "http://aka.ms/vsoext01",
    "support": "http://aka.ms/vsarcontact",
    "repository": "http://aka.ms/vsoext01",
    "issues": "http://aka.ms/vsoext01",
    "learn": "http://blogs.msdn.com/b/visualstudioalmrangers/archive/2015/07/02/folder-management-visual-studio-online-extension-by-wouter-de-kort.aspx"
  },
  "branding": {
    "color": "rgb(220, 235, 252)",
    "theme": "light"
  },
  "scopes": [
    "vso.code_write",
    "vso.code_manage" 
  ],
  "files": [
    {
      "path": "main.html",
      "addressable": true
    },
    {
      "path": "dialog.html",
      "addressable": true
    },
    {
      "path": "scripts",
      "addressable": true
    },
    {
      "path": "sdk",
      "addressable": true
    },
    {
      "path": "images",
      "addressable": true
    },
    {
      "path": "css",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "addFolder",
      "targets": [
        "ms.vss-code-web.source-tree-item-menu"
      ],
      "type": "ms.vss-web.action",
      "properties": {
        "text": "Create a new folder",
        "title": "New Folder",
        "icon": "images/VSO-Folder-16x.png",
        "groupId": "editing",
        "uri": "main.html"
      }
    },
    {
      "id": "createNewFolderDialog",
      "targets": [
        ".dialog"
      ],
      "type": "ms.vss-web.control",
      "properties": {
        "uri": "dialog.html"
      }
    }
  ]
}