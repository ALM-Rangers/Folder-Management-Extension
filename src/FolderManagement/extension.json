{
  "manifestVersion": 1,
  "id": null,
  "version": "1.1.0",
  "publisher": "alm-rangers",
  "extensionId": "almrangers-vsoextensions-FolderManagement",
  "name": "Folder Management",
  "description": "An extension for Visual Studio Online that adds the ability to create new folders directly from web access in the code view",
  "galleryFlags": [
  ],
  "icons": {
    "wide": "images/VSO-Folder-wide.png",
    "default": "images/VSO-Folder-196x.png"
  },
  "categories": [
    "Productivity"
  ],
  "scopes": [ "vso.code_write", "vso.code_manage" ],
  "files": [
    {
      "path": "main.html",
      "addressable": true
    },
    {
      "path": "dialog.html",
      "addressable": true
    },
    {
      "path": "scripts",
      "addressable": true
    },
    {
      "path": "sdk",
      "addressable": true
    },
    {
      "path": "images",
      "addressable": true
    },
    {
      "path": "css",
      "addressable": true
    }
  ],
  "contributions": [
        {
            "id": "addFolder",
            "targets": [
                "ms.vss-code-web.source-tree-item-menu"
            ],
            "type": "ms.vss-web.action",
            "properties": {
                "text": "Create a new folder",
                "title": "New Folder",
                "icon": "images/VSO-Folder-16x.png",
                "groupId": "editing",
                "uri": "main.html"
            }
        },
        {
            "id": "createNewFolderDialog",
            "targets": [
                ".dialog"
            ],
            "type": "ms.vss-web.control",
            "properties": {
                "uri": "dialog.html"
            }
        }
    ]
}