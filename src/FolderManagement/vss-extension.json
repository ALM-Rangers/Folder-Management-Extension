{
  "manifestVersion": 1,
  "id": "FolderManagement",
  "version": "1.1.4",
  "publisher": "ms-devlabs",
  "name": "Folder Management",
  "description": "Quickly create a folder in your Visual Studio Team Services source repositories from the web. No need to clone the repository locally or install extra tools.",
  "public": true,
  "icons": {
    "default": "images/VSO-Folder-196x.png"
  },
  "categories": [
    "Code"
  ],
  "tags": [
    "code",
    "folder folders",
    "directories directory",
    "create",
    "Git",
    "TFVC"
  ],
  "content": {
    "details": {
      "path": "overview.md"
    },
    "license": {
      "path": "license.txt"
    }
  },
  "links": {
    "getstarted": {
      "uri": "http://aka.ms/vsoext1"
    },
    "support": {
      "uri": "http://aka.ms/vsarcontact"
    },
    "repository": {
      "uri": "http://aka.ms/vsoext1"
    },
    "issues": {
      "uri": "https://github.com/ALM-Rangers/VSO-Extension-FolderManagement/issues"
    },
    "learn": {
      "uri": "http://blogs.msdn.com/b/visualstudioalmrangers/archive/2015/07/02/folder-management-visual-studio-online-extension-by-wouter-de-kort.aspx"
    }
  },
  "branding": {
    "color": "rgb(51,65,133)",
    "theme": "dark"
  },
  "scopes": [
    "vso.code_write",
    "vso.code_manage" 
  ],
  "files": [
    {
      "path": "main.html",
      "addressable": true
    },
    {
      "path": "dialog.html",
      "addressable": true
    },
    {
      "path": "scripts",
      "addressable": true
    },
    {
      "path": "sdk",
      "addressable": true
    },
    {
      "path": "images",
      "addressable": true
    },
    {
      "path": "css",
      "addressable": true
    }
  ],
  "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
  "contributions": [
    {
      "id": "addFolder",
      "targets": [
        "ms.vss-code-web.source-tree-item-menu"
      ],
      "type": "ms.vss-web.action",
      "properties": {
        "text": "Create a new folder",
        "title": "New Folder",
        "icon": "images/VSO-Folder-16x.png",
        "groupId": "editing",
        "uri": "main.html"
      }
    },
    {
      "id": "createNewFolderDialog",
      "targets": [
        ".dialog"
      ],
      "type": "ms.vss-web.control",
      "properties": {
        "uri": "dialog.html"
      }
    }
  ]
}